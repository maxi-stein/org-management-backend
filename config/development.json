{
  "env": "development",

  "server": {
    "url": "http://localhost:4000",
    "maxResultsLimit": 1000,
    "port": 4000
  },

  "auth": {
    "token": {
      "issuer": "Org-Management-API",
      "algorithm": "RS256"
    },
    "key": "base-api-local",
    "passwordTtl": 3600
  },

  "rateLimit": {
    "window": "1m",
    "requests": 1000
  },

  "mongo": {
    "db": "org-management",
    "url": "mongodb://localhost/org-management"
  },

  "logger": {
    "sentry": {
      "dsn": ""
    },
    "console": {
      "level": "debug",
      "timestamp": true,
      "handleExceptions": true,
      "humanReadableUnhandledException": true,
      "colorize": true
    }
  },

  "fixerAPI": {
    "apiKey": "rmipUu68oZ25VD30HJWh7SfOxRfqVu9o"
  }
}
