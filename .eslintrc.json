{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "camelcase": "warn",
    "consistent-return": "warn",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "import/no-unresolved": "warn",
    "import/no-named-as-default": "warn",
    "no-console": "warn"
  }
}
